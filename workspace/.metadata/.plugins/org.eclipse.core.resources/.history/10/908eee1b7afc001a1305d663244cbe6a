package ex03_array_quiz;

public class Quiz10 {

	public static void main(String[] args) {
		
		// 문제. 전체 구구단의 결과만
		// 2차원 배열 a에 저장하고, 출력하시오.
		// 실행 예)
		// 2 3 4 ... 9
		// 4 6 8 ... 18
		// ...
		// 18 27 36 ... 81
		
		int[][] a = new int[9][9];
		
		for(int i = 0; i<a.length; i++) {
			for(int j=0; j<a[i].length; j++) {
				for(int b = 2; b<10; b++) {
					for(int c=1; c<10; c++) {
						a[i][j] = b * c;
					}
				}
			}
			System.out.println();
		}
		for(int i = 0; i<a.length; i++) {
			for(int j=0; j<a[i].length; j++) {
				System.out.print(a[i][j]);
				}
			
			System.out.println();
		}
		

	}

}

package ex03_array_quiz;

public class Quiz15 {

	public static void main(String[] args) {
		
		// 문제. 5000원짜리 로또
		int[][] lotto = new int[6][5];
		int a = 1;

		for(int i=0; i < lotto.length; i++) {
			for(int j = 0; j< lotto[i].length; j++) {
				lotto[i][j] = a;
				a++;
			}
		}
		
		
		
	}

}

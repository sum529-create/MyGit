package ex03_method;

import java.util.Date;

class Person{
	
	// field
	String name;
	Date date;
	double height;
	double weight;
	
	// method
	
	// 1. 필드 정보 저장 메소드
	// 1) 결과 타입 : void
	// 2) 메소드명 : setInfo'
	// 3) 매개변수 : String n, double h, double w
	
	void setInfo(String n, double h, double w) {
		name = n;
		date = new Date();
		weight = w;
	}
	
	
}



public class Ex02_Person {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

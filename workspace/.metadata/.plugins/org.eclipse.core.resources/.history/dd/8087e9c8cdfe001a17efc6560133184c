package ex01_oop;

class Book{
	// field
	String title;
	String author;
	
	// constructor
	
	// method
	
	
}

class Person{
	// field
	String name;
	Book book;
	
}

public class Ex03_Person {

	public static void main(String[] args) {
		
		Person p1 = new Person("제임스", "자바책", "김영희");
		Person p2 = new Person("앨리스", "홍길동전");
		Person p3 = new Person("데이빗", new Book("스프링5", "김철수"));
		
		

	}

}

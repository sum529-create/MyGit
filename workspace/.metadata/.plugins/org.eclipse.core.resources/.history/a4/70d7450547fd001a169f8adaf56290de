package ex02_user_type;

public class MainClass {

	public static void main(String[] args) {
		
		// Computer 생성
		Computer myCom = new Computer();
		myCom.model = "GRAM";
		myCom.manufacturer = "LG";
		myCom.price = 180;
		
		// book 생성
		Book myBook = new Book();
		myBook.title = "스프링5";
		myBook.author = "최범규";
		myBook.price = 26500;
		myBook.amount = 5000;
		myBook.isBestSeller = myBook.amount >= 1000;
		
		// person 생성
		Person p = new Person();
		p.name = "최범규";
		p.age = 26500;
		p.height = 5000;
		p.isAdult = p.age >= 20;
		p.computer = myCom;
		p.book = myBook;
		

	}

}

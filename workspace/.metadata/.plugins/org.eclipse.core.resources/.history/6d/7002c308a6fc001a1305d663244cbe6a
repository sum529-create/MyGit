package ex03_array_quiz;

public class Quiz14 {

	public static void main(String[] args) {
		// 문제. 빙고
		// 5 x 5
		// 1. 1 ~ 25 사이 정수를 2차원 배열 bingo에 순차적으로 저장
		// 2. 섞는다.
		//	1) 총 25번의 교환이 발생된다.
		// 	2) bingp[0][0] <-> 랜덤
		// 	   bingp[0][1] <-> 랜덤
		//		...
		// 	   bingo[4][4] <-> 랜덤
		
		int rand = (int)(Math.random()*25)+1;
		int[][] bingo = new int[5][5];
		int[] a = new int[25];
		
		
//		for(int i=0; i < bingo.length; i++) {
//			
//				for(int j = 0; j< bingo[i].length; j++) {
//				
//					bingo[i][j] = a;
//					System.out.println(bingo[i][j]);
//				}
//		}
		for(int b=1; b<26; b++) {
			for(int i=0; i < bingo.length; i++) {
				a[i] = b;
			}
		}
		for(int i=0; i < bingo.length; i++) {
			System.out.println(a[i]);
		}
			

	}

}

package ex03_operator;

public class Ex02_operator {

	public static void main(String[] args) {
		/*
		 * 4. 관계 연산자
		 * 		1) >	: 초과, 크다
		 * 		2) >=	: 이상, 크거나 같다
		 * 		3) <	: 미만, 작다
		 * 		4) <=	: 이하, 작거나 같다
		 * 		5) !=	: 같지 않다
		 * 		6) ==	: 같다 (primitive type)
		 * 			equals : 같다 (reference type)
		 * 		7) 결과 : true 또는 false
		 */
		
		int a = 10;
		int b = 11;
		
		System.out.println(a > b); 		// false
		System.out.println(a >= b); 	// false
		System.out.println(a < b); 		// true
		System.out.println(a <= b);		// true
		System.out.println(a == b);		// false
		System.out.println(a != b);		// true
		
	}

}

package ex03_array_quiz;

import java.util.Random;

public class Quiz07 {

	public static void main(String[] args) {
		// 문제. 윷 던지기
		// 실행 예)
		// "도", 1칸 이동한다.
		// "계", 2칸 이동한다.
		// "걸", 3칸 이동한다.
		// "윷", 4칸 이동한다.
		// "윷", "도", 5칸 이동한다.
		// "모", "윷", "도", 10칸 이동한다.
		
		String[] st = {"도", "계", "걸", "윷", "모"};
		
		int rand = (int)(Math.random()*5)+1;
		
		String a = st[rand];
		System.out.println(a);
		

	}

}
